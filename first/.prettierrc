{
  "printWidth": 120,
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "importOrder": ["<THIRD_PARTY_MODULES>", "@/(.*)", "^../(.*)", "^[./]"],
  "importOrderSeparation": true,
  "importOrderCaseInsensitive": true,
  "importOrderSortSpecifiers": true,
  "jsxSingleQuote": true,
  "bracketSameLine": false
}
